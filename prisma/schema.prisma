datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model products {
  product_id     String  @id @default(uuid())
  name           String
  category       String? // Region
  country        String?
  price          Int?
  district       String? // District
  sub_district   String? // Sub-district
  producer       String? // Producer
  varetype       String? // Type
  lukt           String? // lukt
  smak           String? // Taste
  farge          String? // Color
  metode         String? // Method
  inneholder     String? // Contains
  emballasjetype String? // Packaging type
  korktype       String? // Cork type
  utvalg         String? // Selection
  grossist       String? // Wholesaler
  transportor    String? // Transporter
  imageSmall     String?
  imageMain      String?
}